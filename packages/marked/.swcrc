{
  "$schema": "https://swc.rs/schema.json",
  "jsc": {
    "target": "es2024",
    "parser": { "syntax": "typescript", "decorators": true },
    "transform": { "decoratorVersion": "2022-03" },
    "externalHelpers": true,
    "experimental": {
      "plugins": [
        [
          "swc-plugin-gem",
          {
            "autoImport": {
              "extends": "gem",
              "members": {}
            },
            "autoImportDts": true
          }
        ]
      ]
    }
  }
}
